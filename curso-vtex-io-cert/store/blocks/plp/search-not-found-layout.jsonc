{
    "search-not-found-layout":{
        "children":[
            "flex-layout.row#not-found-title",
            "flex-layout.row#not-found-subtitle",
            "flex-layout.row#suggestions-carousel"
        ]
    },
    "flex-layout.row#not-found-title":{
        "props":{
            "horizontalAlign":"center"
        },
        "children":[
            "rich-text#not-found", 
            "search-title.v2" 
        ]
    },
    "rich-text#not-found":{
        "props":{
            "textAlignment": "CENTER",
            "textPosition": "CENTER",
            "text": "No hay resultados para la b√∫squeda: ",
            "textColor": "c-on-active",
            "font": "t-heading-3"
        }
