    "flex-layout.row#not-found-subtitle":{
        "props":{
            "horizontalAlign":"center"
        },
        "children":["rich-text#subtitle-not-found"]
    },
    "rich-text#subtitle-not-found":{
        "props":{
            "blockClass":"not-found-subtitle",
            "textAlignment": "CENTER",
            "textPosition": "CENTER",
            "text": "PRODUCTOS QUE TE PUEDEN INTERESAR",
            "textColor": "c-muted-1",
            "font": "t-heading-4"
        }
    },

    "flex-layout.row#suggestions-carousel":{
        "props":{
            "marginTop":7,
            "marginBottom":7
        },
        "children":[ "list-context.product-list#new-carousel" ]
    }
